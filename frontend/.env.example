# API Configuration
REACT_APP_API_URL=http://localhost:3001
REACT_APP_WS_URL=ws://localhost:3001

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# Theme Configuration
REACT_APP_DEFAULT_THEME=light

# Chart Configuration
REACT_APP_CHART_REFRESH_RATE=5000
REACT_APP_MAX_DATA_POINTS=100

# MQTT Configuration
REACT_APP_MQTT_HOST=localhost
REACT_APP_MQTT_PORT=9001
REACT_APP_MQTT_PATH=/mqtt
